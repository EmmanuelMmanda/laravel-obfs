{
    "name": "mmanda/laravel-obfs",
    "description": "A Simple wrapper for YakPro-po obfuscator for Laravel Framework ",
    "type": "library",
    "require": {
        "php": "^7.1|^8.0",
        "pmdunggh/yakpro-po": "dev-master",
        "nikic/php-parser": "^4.x-dev",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "license": "MIT",   
    "autoload": {
        "psr-4": {
            "Mmanda\\LaravelObfs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mmanda\\LaravelObfs\\Providers\\ObfuscateServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Emmanuel-Mmanda",
            "email": "luneya17@gmail.com"
        }
    ],

    "minimum-stability": "dev",
    "prefer-stable": true
}
